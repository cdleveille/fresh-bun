{
  "name": "fresh-bun",
  "version": "1.0.0",
  "description": "ðŸ‘‹",
  "author": "Chris Leveille",
  "license": "MIT",
  "type": "module",
  "module": "src/server/index.ts",
  "scripts": {
    "prepare": "bun run lefthook.ts",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "check": "biome check .",
    "tsc": "tsc -p tsconfig.json",
    "concurrent": "concurrently --prefix \"[{name}]\" -c \"bgBlue.bold,bgGreen.bold\"",
    "dev": "bun run concurrent --names \"server,client\" \"bun run --hot src/server/index.ts\" \"bunx --bun vite\"",
    "compile": "bun build --compile --minify --sourcemap --bytecode --target bun --outfile main src/server/index.ts",
    "bundle": "bunx --bun vite build && bun run src/client/helpers/workbox.ts",
    "build": "bun run compile && bun run bundle",
    "start": "bun run src/server/index.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.6",
    "@evilmartians/lefthook": "^1.11.16",
    "@tanstack/react-router-devtools": "^1.125.6",
    "@tanstack/router-plugin": "^1.125.6",
    "@types/bun": "latest",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "babel-plugin-react-compiler": "^19.1.0-rc.2",
    "concurrently": "^9.2.0",
    "typescript": "^5.8.3",
    "vite": "^7.0.2",
    "vite-plugin-static-copy": "^3.1.0",
    "vite-plugin-svgr": "^4.3.0",
    "vite-tsconfig-paths": "^5.1.4",
    "workbox-build": "^7.3.0"
  },
  "dependencies": {
    "@elysiajs/eden": "^1.3.2",
    "@elysiajs/static": "^1.3.0",
    "@elysiajs/swagger": "^1.3.1",
    "@tanstack/react-query": "^5.81.5",
    "@tanstack/react-router": "^1.125.6",
    "elysia": "^1.3.5",
    "elysia-helmet": "^3.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hot-toast": "^2.5.2",
    "zustand": "^5.0.6"
  }
}
