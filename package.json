{
  "name": "fullstack-bun",
  "description": "Bun/TypeScript monorepo template.",
  "author": "Chris Leveille <cdleveille@gmail.com",
  "license": "MIT",
  "module": "src/server/index.ts",
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "lint": "bunx eslint --ext .js,.jsx,.ts,.tsx . && bunx prettier --check .",
    "fix": "bunx eslint --fix --ext .js,.jsx,.ts,.tsx . && bunx prettier --write .",
    "db:up": "docker compose -f \".fullstack-bun-dev-container/docker-compose.yml\" up -d --build",
    "build": "bun scripts/build.ts && workbox injectManifest workbox-config.json",
    "build:prod": "bun scripts/build.ts BUN_ENV=production && workbox injectManifest workbox-config.json",
    "dev": "bun --hot src/server/index.ts",
    "start": "bun src/server/index.ts"
  },
  "devDependencies": {
    "@types/minify": "^9.1.1",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "bun-types": "latest",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.3",
    "minify": "^10.5.2",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "typescript": "^5.2.2",
    "workbox-cli": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0"
  },
  "dependencies": {
    "hono": "^3.7.2",
    "mongoose": "^7.5.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2"
  }
}
