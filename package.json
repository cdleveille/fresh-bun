{
  "name": "fullstack-bun",
  "version": "1.0.0",
  "description": "ðŸ‘‹",
  "author": "Chris Leveille",
  "license": "MIT",
  "type": "module",
  "module": "src/server/index.ts",
  "scripts": {
    "prepare": "bun run lefthook.ts",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "check": "biome check .",
    "tsc": "tsc -p tsconfig.json",
    "build": "bunx --bun vite build && bun run src/server/helpers/workbox.ts",
    "compile": "bun build --compile --minify --sourcemap --bytecode --target bun --outfile main src/server/index.ts ",
    "deploy": "flyctl deploy",
    "dev": "concurrently --prefix \"[{name}]\" --names \"server,client\" -c \"bgBlue.bold,bgGreen.bold\" \"bun run --hot src/server/index.ts\" \"bunx --bun vite\"",
    "start": "bun run src/server/index.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@evilmartians/lefthook": "^1.11.13",
    "@types/bun": "latest",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react": "^4.5.0",
    "babel-plugin-react-compiler": "^19.1.0-rc.2",
    "concurrently": "^9.1.2",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-static-copy": "^3.0.0",
    "vite-plugin-svgr": "^4.3.0",
    "vite-tsconfig-paths": "^5.1.4",
    "workbox-build": "^7.3.0"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.3.3",
    "@elysiajs/eden": "^1.3.2",
    "@elysiajs/static": "^1.3.0",
    "@elysiajs/swagger": "^1.3.0",
    "@tanstack/react-query": "^5.77.2",
    "elysia": "^1.3.3",
    "elysia-helmet": "^3.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1"
  }
}
