{
  "name": "fresh-bun",
  "version": "1.0.0",
  "description": "ðŸ‘‹",
  "author": "Chris Leveille",
  "license": "MIT",
  "type": "module",
  "module": "src/server/index.ts",
  "scripts": {
    "prepare": "bun run lefthook.ts",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "check": "biome check .",
    "tsc": "tsc -p tsconfig.json",
    "concurrent": "concurrently --prefix \"[{name}]\" -c \"bgBlue.bold,bgGreen.bold\"",
    "dev": "bun run concurrent --names \"server,client\" \"bun run --hot src/server/index.ts\" \"bunx --bun vite\"",
    "compile": "bun build --compile --minify --sourcemap --bytecode --target bun --outfile bin/main src/server/index.ts",
    "bundle": "bunx --bun vite build && bun run src/client/helpers/workbox.ts",
    "build": "bun run compile && bun run bundle",
    "start": "bun run src/server/index.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@evilmartians/lefthook": "^1.13.1",
    "@tanstack/react-router-devtools": "^1.131.49",
    "@tanstack/router-plugin": "^1.131.49",
    "@types/bun": "^1.2.22",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "babel-plugin-react-compiler": "^19.1.0-rc.1-rc-af1b7da-20250421",
    "concurrently": "^9.2.1",
    "typescript": "^5.9.2",
    "vite": "^7.1.6",
    "vite-plugin-static-copy": "^3.1.2",
    "vite-plugin-svgr": "^4.5.0",
    "vite-tsconfig-paths": "^5.1.4",
    "workbox-build": "^7.3.0"
  },
  "dependencies": {
    "@elysiajs/eden": "^1.4.1",
    "@elysiajs/html": "^1.4.0",
    "@elysiajs/static": "^1.4.0",
    "@elysiajs/swagger": "1.2.2",
    "@tanstack/react-query": "^5.89.0",
    "@tanstack/react-router": "^1.131.49",
    "elysia": "^1.4.6",
    "elysia-helmet": "^3.0.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hot-toast": "^2.6.0",
    "zustand": "^5.0.8"
  }
}
